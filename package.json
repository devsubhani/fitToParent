{
  "name": "fit-to-parent",
  "version": "1.3.2",
  "description": "A tool that will resize a DOM element to fit its parent container while maintaining the original aspect ratio.",
  "scripts": {
    "build": "babel ./src --out-dir ./lib && webpack --progress",
    "dev": "webpack-dev-server --content-base examples/ --watch --open",
    "test": "tap test.js",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funkhaus/fitToParent.git"
  },
  "keywords": [
    "resize",
    "element",
    "container",
    "parent",
    "aspect",
    "ratio",
    "jquery"
  ],
  "author": "Funkhaus",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/funkhaus/fitToParent/issues"
  },
  "homepage": "https://github.com/funkhaus/fitToParent#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "tap": "^10.7.0",
    "uglify-js": "^3.0.27",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  }
}
